{
  "$schema": "http://json-schema.org/schema#",
  "type": "array",
  "items": {
    "type": "array",
    "items": {
      "type": "object",
      "properties": {
        "artist": {
          "type": "object",
          "properties": {
            "mbid": {
              "type": "string"
            },
            "#text": {
              "type": "string"
            }
          },
          "required": [
            "#text",
            "mbid"
          ]
        },
        "streamable": {
          "type": "string"
        },
        "image": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "size": {
                "type": "string"
              },
              "#text": {
                "type": "string"
              }
            },
            "required": [
              "#text",
              "size"
            ]
          }
        },
        "mbid": {
          "type": "string"
        },
        "album": {
          "type": "object",
          "properties": {
            "mbid": {
              "type": "string"
            },
            "#text": {
              "type": "string"
            }
          },
          "required": [
            "#text",
            "mbid"
          ]
        },
        "name": {
          "type": "string"
        },
        "@attr": {
          "type": "object",
          "properties": {
            "nowplaying": {
              "type": "string"
            }
          },
          "required": [
            "nowplaying"
          ]
        },
        "url": {
          "type": "string"
        },
        "date": {
          "type": "object",
          "properties": {
            "uts": {
              "type": "string"
            },
            "#text": {
              "type": "string"
            }
          },
          "required": [
            "#text",
            "uts"
          ]
        }
      },
      "required": [
        "album",
        "artist",
        "image",
        "mbid",
        "name",
        "streamable",
        "url"
      ]
    }
  }
}